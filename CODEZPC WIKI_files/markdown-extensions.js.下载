document.addEventListener("DOMContentLoaded",function(){const collapsibleSections=document.querySelectorAll("details.markdown-details");if(collapsibleSections.length>0){const originalStates=new Map;window.addEventListener("beforeprint",function(){collapsibleSections.forEach(details=>{originalStates.set(details,details.open);details.open=true})});window.addEventListener("afterprint",function(){collapsibleSections.forEach(details=>{if(originalStates.has(details)){details.open=originalStates.get(details)}})})}});