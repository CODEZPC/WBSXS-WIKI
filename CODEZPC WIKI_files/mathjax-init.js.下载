window.MathJax={tex:{inlineMath:[["$","$"]],displayMath:[["$$","$$"]],processEscapes:true,processEnvironments:true},options:{skipHtmlTags:["script","noscript","style","textarea","pre","code"],ignoreHtmlClass:"tex2jax_ignore",processHtmlClass:"tex2jax_process"}};document.addEventListener("DOMContentLoaded",function(){document.addEventListener("content-updated",function(e){if(typeof MathJax!=="undefined"&&e.detail&&e.detail.element){try{MathJax.typesetPromise([e.detail.element])}catch(error){console.error("MathJax processing error:",error)}}})});