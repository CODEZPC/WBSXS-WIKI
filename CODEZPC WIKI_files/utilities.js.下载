function getCurrentDocPath(){console.log("Raw pathname for path processing:",window.location.pathname);const isHomepage=window.location.pathname==="/";if(isHomepage){console.log("Using homepage path");return"pages/home"}let path=window.location.pathname;if(path.startsWith("/")){path=path.substring(1)}if(path.endsWith("/")){path=path.substring(0,path.length-1)}if(path.endsWith(".md")){path=path.substring(0,path.length-3)}console.log("Processed document path:",path);return path||"pages/home"}window.getCurrentDocPath=getCurrentDocPath;